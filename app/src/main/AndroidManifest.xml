<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anibal.neumaticosapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logocs"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logocs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".EleccionActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".AgregarActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".EliminarActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".AvisosActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>